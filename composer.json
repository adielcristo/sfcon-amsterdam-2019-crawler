{
    "name": "adielcristo/symfonycon-2019",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Adiel Cristo",
            "email": "adielcristo@gmail.com",
            "homepage": "https://adielcristo.com"
        }
    ],
    "require": {
        "symfony/console": "^4.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "extra": {
        "symfony": {
            "require": "^4.3"
        }
    }
}
